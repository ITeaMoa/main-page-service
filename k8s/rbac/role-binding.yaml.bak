apiVersion: rbac.authorization.k8s.io/v1
kind: RoleBinding
metadata:
  name: main-service-rolebinding
  namespace: default # Use your namespace if defined
subjects:
  - kind: ServiceAccount
    name: main-service-account
    namespace: default # Use your namespace if defined
roleRef:
  apiGroup: rbac.authorization.k8s.io
  kind: Role
  name: main-service-role
